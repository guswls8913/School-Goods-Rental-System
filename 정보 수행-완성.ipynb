    "        if mode_num=='1':\n",
    "            print('학번을 입력하세요')\n",
    "            d = input()\n",
    "            q.enqueue(d)\n",
    "            a=input_func()\n",
    "            if a[0] not in main_data:\n",
    "                print('그런 물품은 없습니다')\n",
    "            elif main_data[a[0]]-a[1]<0:\n",
    "                print('물품 수량이 부족합니다.')\n",
    "            else:\n",
    "                main_data[a[0]]-=a[1]\n",
    "        elif mode_num=='2':\n",
    "            a=input_func()\n",
    "            if a[1]+main_data[a[0]] > origin_data[a[0]]:\n",
    "                print('수량을 재확인 하세요')\n",
    "            else:\n",
    "                main_data[a[0]]+=a[1]\n",
    "        elif mode_num=='3':\n",
    "            print(main_data)\n",
    "        elif mode_num=='4':\n",
    "            break\n",
    "        else:\n",
    "            print('1,2,3,4 중 하나를 입력해 주세요')\n",
    "\n",
    "def 학번확인():\n", #
